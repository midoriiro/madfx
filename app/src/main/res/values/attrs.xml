<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Visualizer">
        <attr name="normalize" format="boolean"/>
        <attr name="color" format="color"/>
        <attr name="opacity" format="float"/>
        <attr name="density" format="float"/>
        <attr name="gap" format="integer"/>
        <attr name="smooth" format="integer"/>
        <attr name="radius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="Equalizer">
        <attr name="primaryTraceColor" format="color"/>
        <attr name="primaryTraceStrokeWidth" format="dimension"/>
        <attr name="secondaryTraceColor" format="color"/>
        <attr name="secondaryTraceStrokeWidth" format="dimension"/>
        <attr name="gridColor" format="color"/>
        <attr name="gridStrokeWidth" format="dimension"/>
        <attr name="gridPaddingTop" format="dimension"/>
        <attr name="gridPaddingBottom" format="dimension"/>
        <attr name="gridPaddingLeft" format="dimension"/>
        <attr name="gridPaddingRight" format="dimension"/>
        <attr name="labelXColor" format="color"/>
        <attr name="labelXSize" format="dimension"/>
        <attr name="labelXPadding" format="dimension"/>
        <attr name="labelYColor" format="color"/>
        <attr name="labelYSize" format="dimension"/>
        <attr name="labelYPadding" format="dimension"/>
        <attr name="bubbleSize" format="dimension"/>
    </declare-styleable>
</resources>