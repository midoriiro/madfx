<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Knob">
        <attr name="android:enabled" />
        <attr name="traceStrokeWidth" format="dimension"/>
        <attr name="bubbleSize" format="dimension"/>
        <attr name="minimumValue" format="float"/>
        <attr name="maximumValue" format="float"/>
        <attr name="middleValues" format="string"/>
        <attr name="defaultValue" format="float"/>
        <attr name="value" format="float"/>
        <attr name="name" format="string"/>
        <attr name="nameSize" format="dimension"/>
        <attr name="labelSize" format="dimension"/>
        <attr name="showLabels" format="boolean"/>
        <attr name="valueFormatter" format="string"/>
        <attr name="labelFormatter" format="string"/>
        <attr name="gestureOrientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
            <enum name="circular" value="2"/>
        </attr>
        <attr name="traceOrientation" format="enum">
            <enum name="clockwise" value="0"/>
            <enum name="bipolar" value="1"/>
        </attr>
        <attr name="animationDuration"/>
        <attr name="animationInterpolator"/>
        <attr name="animationEvaluator"/>
    </declare-styleable>
</resources>